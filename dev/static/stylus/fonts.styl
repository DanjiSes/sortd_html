@font-face {
  font-family: 'SF UI Display';
  font-style: normal;
  font-weight: normal;
  src: url('/assets/fonts/SFUIDIsplay/SFUIDisplay-Regular.eot?#iefix') format('embedded-opentype'),
  url('/assets/fonts/SFUIDIsplay/SFUIDisplay-Regular.eot'),
  url('/assets/fonts/SFUIDIsplay/SFUIDisplay-Regular.woff') format('woff'),
  url('/assets/fonts/SFUIDIsplay/SFUIDisplay-Regular.ttf') format('truetype'),
  url('/assets/fonts/SFUIDIsplay/SFUIDisplay-Regular.svg#SFUIDisplay-Regular') format('svg');
}

